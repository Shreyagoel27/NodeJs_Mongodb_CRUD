
{{#each users}}
<tr>
                <td>{{this._id}}</td>
				  <td>{{this.name}}</td>
				  <td>{{this.email}}</td>
				  <td>{{this.gender}}</td>
				  <td>{{this.status}}</td>
				  <td>
					  <a href="/update-user?id={{this._id}}" class="btn-border-shadow update">
						  <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
					  </a>
					 <a class="btn border-shadow delete" data-id={{this._id}}  >
                           <span class="text-gradient"><i class="fas fa-times"></i></span>
                      </a>
				  </td>
</tr>
                  
{{/each}}